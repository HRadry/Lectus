<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="webapp.lectus.models.Area" table="area" catalog="lectus" optimistic-lock="version">
        <id name="idArea" type="java.lang.Integer">
            <column name="idArea"/>
            <generator class="identity"/>
        </id>
        <property name="edificio" type="java.lang.String">
            <column name="edificio" length="45" not-null="true"/>
        </property> 
        <property name="telefono" type="java.lang.String">
            <column name="telefono" length="20" not-null="true"/>
        </property>   
    </class>    
</hibernate-mapping>
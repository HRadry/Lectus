<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="webapp.lectus.models.AlumnoLibro" table="alumnolibro" catalog="lectus" optimistic-lock="version">
        <id name="idAlumnoLibro" type="java.lang.Integer">
            <column name="idAlumnoLibro"/>
            <generator class="identity"/>
        </id>
        <property name="idUsuario" type="java.lang.Integer">
            <column name="idUsuario" length="45" not-null="false"/>
        </property>
        <property name="idLibro" type="java.lang.Integer">
            <column name="idLibro" length="45" not-null="false"/>
        </property>   
        <property name="idRevisor" type="java.lang.Integer">
            <column name="idRevisor" length="45" not-null="false"/>
        </property>
    </class>    
</hibernate-mapping>